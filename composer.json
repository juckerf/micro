{
    "name": "gyselroth/micro",
    "description": "Micro PHP Framework",
    "keywords": ["micro", "simple", "php", "lib", "framework"],
    "homepage": "http://www.github.com/gyselroth/micro",
    "license": "MIT",
    "version": "0.0.1",
    "authors": [
        {
            "name": "Raffael Sahli",
            "email": "sahli@gyselroth.net"
        }
    ],
    "autoload": {
        "psr-4": {
            "Micro\\": "src",
            "Micro\\Testsuite\\": "tests"
        }
    },
    "require": {
        "php": ">=7.1",
        "psr/log": "1.*"
    },
    "suggest": {
        "ext-xml": "required if you want to use \\Micro\\Config\\Xml",
        "ext-ldap": "required if you want to use \\Micro\\Ldap"
    },
    "require-dev": {
        "phpunit/phpunit": "5.7.*"
    }
}
